{
    "tags": {
        "CostCenter": {
            "tag_key": {
                "@@assign": "CostCenter"
            },
            "tag_value": {
                "@@assign": [
                    "100",
                    "200"
                ]
            }
        },
        "map-migrated": {
            "tag_value": {
                "@@assign": [
                    "mig1234abc"
                ]
            },
